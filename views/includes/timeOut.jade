div.user-que(ng-show="data.showUserTimedOut")
	h3.que-header SORRY, YOU TIMED OUT

	img.down-arrow(src="/images/que_down_arrow.png")
	div.container.timed-out.row
		p.mdm.place-in-line THANKS FOR STOPPING BY
		button.btn.btn-primary.span(ng-click="playAgain()") 
			span.submit-form Play again
		p.exercise-inst REMEMBER, THE BEST WAY TO PREVENT COLON CANCER IS TO STAY LEAN, EAT CLEAN AND GET SCREENED.
		p.exercise-inst DAYS TO PLAY: #
			span= daysLeft
			br 
			| TEXT 9999 TO DONATE $5 NOW
		p.exercise-inst SHARE THE FACTS ABOUT 
			br 
			| COLORECTAL CANCER
		div.share-box
			span.share.facebook(ng-click="share('facebook')") share 
				img(src="/images/share_facebook.png")
			span.share.twitter(ng-click="share()") share 
				img(src="/images/share_twitter.png")
		p.exercise-inst learn more at 
			a(href="") COLONCANCERCHALLENGE.ORG

